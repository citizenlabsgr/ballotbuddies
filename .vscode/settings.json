{
  "files.exclude": {
    ".cache/": true,
    ".coverage": true,
    ".pytest_cache": true,
    ".venv/": true,
    "*.egg-info/": true,
    "*.log": true,
    "**/__pycache__/": true,
    "htmlcov/": true,
    "staticfiles/": true
  },
  "python.pythonPath": ".venv/bin/python",
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.linting.pylintPath": ".venv/bin/pylint",
  "python.linting.pylintArgs": ["--rcfile", ".pylint.ini"],
  "python.linting.mypyEnabled": true,
  "python.linting.mypyPath": ".venv/bin/mypy",
  "python.linting.mypyArgs": ["--config-file", ".mypy.ini"],
  "python.formatting.provider": "black",
  "python.formatting.blackPath": ".venv/bin/black",
  "editor.formatOnSave": true,
  "cSpell.words": [
    "asdict",
    "asgi",
    "ballotbuddies",
    "bugsnag",
    "circleci",
    "cleandata",
    "corsheaders",
    "createdb",
    "direnv",
    "Friendo",
    "gunicorn",
    "herokuapp",
    "htmlcov",
    "HTMX",
    "Kentwood",
    "levelname",
    "mandrillapp",
    "maxsplit",
    "michiganelections",
    "ngrok",
    "PYTHONUNBUFFERED",
    "Rosalynn",
    "runserver",
    "Ulrich",
    "Unfollowing",
    "Voterface",
    "Voterson",
    "Votey",
    "whitenoise",
    "wsgi",
    "zipcodes"
  ]
}
